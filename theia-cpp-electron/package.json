{
    "private": true,
    "name": "@theia/cpp-electron-example",
    "version": "0.0.1",
    "description": "Theia C/C++ Electron example",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theia-ide/theia-apps.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/theia-ide/theia-apps/issues"
    },
    "homepage": "https://github.com/theia-ide/theia-apps/theia-cpp-electron",
    "theia": {
        "target": "electron",
        "frontend": {
            "config": {
                "applicationName": "Theia C/C++ Electron Example",
                "preferences": {
                    "files.enableTrash": false,
                    "cpp.clangdExecutable": "clangd",
                    "cpp.clangdArgs": "",
                    "cpp.clangTidy": false
                }
            }
        }
    },
    "scripts": {
        "build": "yarn theia rebuild:electron; yarn theia build",
        "start": "yarn theia start"
    },
    "dependencies": {
        "@theia/callhierarchy": "latest",
        "@theia/console": "latest",
        "@theia/core": "latest",
        "@theia/cpp": "latest",
        "@theia/cpp-debug": "latest",
        "@theia/debug": "latest",
        "@theia/debug-nodejs": "latest",
        "@theia/editor": "latest",
        "@theia/editor-preview": "latest",
        "@theia/electron": "latest",
        "@theia/file-search": "latest",
        "@theia/filesystem": "latest",
        "@theia/getting-started": "latest",
        "@theia/git": "latest",
        "@theia/json": "latest",
        "@theia/keymaps": "latest",
        "@theia/languages": "latest",
        "@theia/markers": "latest",
        "@theia/merge-conflicts": "latest",
        "@theia/messages": "latest",
        "@theia/metrics": "latest",
        "@theia/mini-browser": "latest",
        "@theia/monaco": "latest",
        "@theia/navigator": "latest",
        "@theia/outline-view": "latest",
        "@theia/output": "latest",
        "@theia/plantuml": "latest",
        "@theia/plugin": "latest",
        "@theia/plugin-ext": "latest",
        "@theia/plugin-ext-vscode": "latest",
        "@theia/preferences": "latest",
        "@theia/preview": "latest",
        "@theia/process": "latest",
        "@theia/scm": "latest",
        "@theia/search-in-workspace": "latest",
        "@theia/task": "latest",
        "@theia/terminal": "latest",
        "@theia/textmate-grammars": "latest",
        "@theia/tslint": "latest",
        "@theia/typehierarchy": "latest",
        "@theia/typescript": "latest",
        "@theia/userstorage": "latest",
        "@theia/variable-resolver": "latest",
        "@theia/workspace": "latest",
        "typescript": "latest",
        "@theia/node-pty": "0.7.8-theia004",
        "nsfw": "1.2.5",
        "find-git-repositories": "0.1.1"
    },
    "resolutions": {
        "vscode-languageserver-protocol": "3.15.0-next.9",
        "vscode-languageserver-types": "3.15.0-next.5",
        "**/vscode-json-languageserver/**/vscode-languageserver": "6.0.0-next.1"
    },
    "devDependencies": {
        "@theia/cli": "latest"
    }
}
